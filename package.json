{
  "name": "nashi-lazy",
  "version": "0.1.0-alpha.0",
  "description": "nashi.lazy is a library for lazy loading images. Powered by nashi.",
  "author": "wibus-wee <wibus@qq.com>",
  "license": "AGPLv3",
  "homepage": "https://github.com/wibus-wee/nashi.lazy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wibus-wee/nashi.lazy.git"
  },
  "issues": "https://github.com/wibus-wee/nashi.lazy/issues",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache --fix"
    ]
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bump": {
    "leading": [
      "git pull --rebase",
      "npm run build"
    ],
    "changelog": true,
    "publish": true
  },
  "scripts": {
    "prepare": "husky install",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bump",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "dev:html": "vite"
  },
  "devDependencies": {
    "@innei-util/eslint-config-ts": "0.8.2",
    "@innei-util/prettier": "0.8.2",
    "@innei/bump-version": "^1.5.0",
    "@innei/eslint-config-ts": "0.9.7",
    "@types/node": "18.11.18",
    "eslint": "8.31.0",
    "esno": "0.16.3",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "lodash-es": "^4.17.21",
    "pnpm": "7.18.2",
    "prettier": "2.8.3",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "unbuild": "1.0.2",
    "vite": "4.0.4",
    "vitest": "0.26.3"
  },
  "dependencies": {
    "@akrc/nashi": "^0.2.3"
  }
}
